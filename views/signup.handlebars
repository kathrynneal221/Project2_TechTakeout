<div class="card">
    <div class="card-body">
        <h3 class="card-header">Sign Up</h3>
        <form id="signup-form">
            <div class="form-group">
                <label for="email-signup">Email</label>
                <input type="text" class="form-control" id="email-signup" autofocus="true" class
                    aria-describedby="email">
            </div>
            <div class="form-group">
                <label for="password-signup">Password</label>
                <input type="password" class="form-control" autofocus="true" id="password-signup">
            </div>
            <button type="submit" class="btn btn-secondary btn-lg">Sign Up</button>
        </form>

    </div>
</div>


<script src="/js/signup.js"></script>